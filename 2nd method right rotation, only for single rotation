#include <iostream>

using namespace std;

int main()
{
    int i,j,temp=0,b[100];
    int arr[100]={1,2,3,4,5};
    for(i=0;i<5;i++)
    {
        cout<<arr[i];
        
    }
    
    temp=arr[4];
    for(i=0;i<5;i++)
    {
        b[i+1]=arr[i];
    }
    b[0]=temp;
    
    cout<<"\n";
    for(i=0;i<5;i++)
    {
        cout<<b[i];
        
    }
}
